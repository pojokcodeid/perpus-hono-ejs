<div class="container mt-3">
  <% if(message) { %>
    <div class="alert alert-<%- type %> alert-dismissible fade show" role="alert">
      <%- message %>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
  <%}%>
  <h1><%- title %></h1>
</div>

